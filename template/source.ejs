<!doctype html>
<html lang="en">
<head>
    <title><%=pgk.name%>源码-<%=relativePath%></title>
    <meta charset="UTF-8">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="Expires" content="0">
    <meta http-equiv="Pragma" content="no-cache">
    <meta http-equiv="Cache-control" content="no-cache">
    <meta http-equiv="Cache" content="no-cache">
    <meta name="keywords" content=""/>
    <meta name="description" content=""/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0,user-scalable=no" />
    <meta name="format-detection" content="telephone=no">
    <meta name="format-detection" content="email=no"  />
    <meta name="apple-mobile-web-app-capable" content="yes"/>
    <meta name="apple-mobile-web-app-capabale", content="yes"/>
    <link href="../css/layout.css" type="text/css" rel="stylesheet" />
	<link href="../font/font.css" type="text/css" rel="stylesheet" />
    <link href="../css/prettify-jsdoc.css" type="text/css" rel="stylesheet" />
    <link href="../css/prettify-tomorrow.css" type="text/css" rel="stylesheet" />
</head>
<body class="layout layout-source">
<div class="layout-header">
    <h3 class="title"><%=name%><span class="version"></span></h3>
    <ul class="nav">
        <% 
            var repository = pgk.repository || {};
            var type=repository.type;
            var url = repository.url || '';
            if (url.replace(/\s/g,'')!=''){
        %>
        <li class="nav-item">
            <a class="repository" target="_blank" href="<%=url.replace('git:','http:').replace('git@','http://')%>" >
					<span><%=type%></span>
				</a>
        </li>
        <%
            }
        %>
    </ul>
</div>
<div class="layout-content">
    <div class="file-source">
    <div class="file-header"><%=relativePath%></div>
    <pre class="prettyprint source linenums"><%-fullCode%></pre>
    </div>
</div>
<script type="text/javascript" src="../scripts/prettify.js"></script>
<script type="text/javascript" src="../scripts/lang-css.js"></script>
<script> prettyPrint(); </script>
<script type="text/javascript" src="../scripts/linenumber.js"></script>
</body>
</html>

